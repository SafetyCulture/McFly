    "name": "Tristan Davey",
    "email": "tristan.davey@safetyculture.io"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/SafetyCulture/mcfly.git"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --compilers coffee:coffee-script/register --reporter spec --recursive -s 120",
