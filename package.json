    "stream-transform": "0.0.7"
  },
  "devDependencies": {
    "coffee-coverage": "^1.0.0",
    "coffeelint": "^1.15.7",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3"
  }
}
